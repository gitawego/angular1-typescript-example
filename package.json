{
  "name": "kpler-js-test-hongbo-lu",
  "version": "1.1.0",
  "description": "kpler js test",
  "keywords": [
    "angular",
    "typescript",
    "angular material"
  ],
  "author": "Hongbo LU <gitawego@gmail.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/MarcBT/kpler-js-test-hongbo-lu",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/MarcBT/kpler-js-test-hongbo-lu"
  },
  "scripts": {
    "test": "npm run clean:coverage && npm run build:dev && karma start",
    "test:ci": "npm test && npm run coverage:remap",
    "coverage": "npm test && npm run coverage:remap",
    "coverage:remap": "remap-istanbul --input ./coverage/coverage-final.json --output ./coverage/lcov-report -t html && remap-istanbul --input ./coverage/coverage-final.json --output ./coverage/lcov.info -t lcovonly",
    "coverage:view": "google-chrome-stable ./coverage/lcov-report/index.html",
    "protractor": "protractor",
    "clean:dist": "rimraf dist",
    "clean:devcache": "rimraf .awcache",
    "clean:coverage": "rimraf coverage",
    "selenium": "bash ./docker/selenium/selenium.sh",
    "build:app:dev": "webpack --config config/webpack.dev.js --profile --progress",
    "build:app": "NODE_ENV=production webpack --config config/webpack.prod.js --bail",
    "build:app:watch": "npm run clean:devcache && webpack --config config/webpack.dev.js --profile --watch",
    "build:dev": "npm run clean:dist && npm run build:app:dev",
    "build": "npm run clean:dist && npm run build:app",
    "server:dev": "npm run clean:devcache && webpack-dev-server --config config/webpack.dev.js --inline",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update --chrome",
    "webdriver:start": "./node_modules/.bin/webdriver-manager -- start",
    "webdriver:start:d": "./node_modules/.bin/webdriver-manager start --detach",
    "webdriver:stop": "./node_modules/.bin/webdriver-manager shutdown"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "@types/angular": "^1.6.2",
    "@types/angular-material": "^1.1.44",
